{
  "name": "hamon-upload-utility",
  "version": "0.1.0-beta",
  "private": true,
  "description": "HAMon ETS configuration upload utility",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "dev": "concurrently --kill-others \"yarn watch:frontend\" \"yarn watch:backend\" --names \"FRONTEND,BACKEND\"",
    "postinstall": "yarn build",
    "start": "webpack && node backend.js",
    "watch:backend": "nodemon backend.js",
    "watch:frontend": "webpack --watch --mode=development"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.8.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sax": "^1.2.4",
    "style-loader": "^3.3.3",
    "unzipper": "^0.10.11",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^2.0.15",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
